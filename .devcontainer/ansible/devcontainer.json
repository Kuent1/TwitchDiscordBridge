{
    "name": "Ansible",
    "dockerFile": "Dockerfile",
    "customizations": {
        "vscode": {
            "extensions": [
                // List of extensions to install in the dev container
                "ms-python.python",
                "ms-vscode.vscode-typescript-tslint-plugin"
            ]
        }},
    "forwardPorts": [
        // List of ports to forward from the container to the host
        3000,
        8080
    ],
    "postCreateCommand": "echo 'Dev container created!'"
}
